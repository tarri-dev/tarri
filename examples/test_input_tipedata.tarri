titikawal {
    _tipe_data = masukkan(_tipe_data, "masukkan :")

    jika tipedata(_tipe_data) == "kata" {
        cetak "tipe data itu adalah kata"
    } ataujika tipedata(_tipe_data) == "angka" {
        cetak "tipe data itu adalah angka"
    } ataujika tipedata(_tipe_data) == "desimal" {
        cetak "tipe data itu adalah desimal"
    } lainnya {
        cetak "tipe data tidak diketahui"
    }
}


titikawal {
    _data = masukkan(_data, "masukkan sesuatu: ")

    jika tipedata(_data) == "angka" {
        cetak "kamu masukkan angka {_data}"
    } ataujika tipedata(_data) == "desimal" {
        cetak "kamu masukkan desimal {_data}"
    } ataujika tipedata(_data) == "kata" {
        cetak "kamu masukkan kata '{_data}'"
    } lainnya {
        cetak "tipe data tidak diketahui"
    }
}
