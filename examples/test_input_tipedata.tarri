titikawal{

    masukkan(_cek,"masukkan : ")


    jika tipedata(_cek) == "angka" {
        cetak "kamu memasukkan angka, {_cek}"
    }ataujika tipedata(_cek) == "kata" {
        cetak "kamu memasukkan kata, {_cek}"
    }ataujika tipedata(_cek) == "desimal" {
        cetak "kamu memasukkan desimal, {_cek}"
    }ataujika tipedata(_cek) == "logika" {
        cetak "kamu memasukkan logika, {_cek}"
    }lainnya{
        cetak "tipe data tidak diketahui"
    }
}